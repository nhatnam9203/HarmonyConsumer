{
  "name": "HarmonyPay",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --scheme=\"Harmony Pay\" --simulator=\"iPhone 11 Pro\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write ./src && prettier --check ./src",
    "clean": "rm -rf node_modules && rm -rf yarn.lock && rm -rf $TMPDIR/react-* && watchman watch-del-all",
    "pod:clean": "cd ios && pod deintegrate && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && rm -rf Podfile.lock",
    "push:ios-production": "appcenter codepush release-react -a HarmonyPayment/harmonypay-iOS -d Production -t 3.9.12 --description fixBug",
    "push:ios-staging": "appcenter codepush release-react -a HarmonyPayment/harmonypay-iOS -d Staging -t 1.0.8 --description test",
    "push-clean:ios-production": "appcenter codepush deployment clear -a HarmonyPayment/harmonypay-iOS Production",
    "push-clean:ios-staging": "appcenter codepush deployment clear -a HarmonyPayment/harmonypay-iOS Staging",
    "push:android-production": "appcenter codepush release-react -a HarmonyPayment/harmonypay-Android -d Production -t 2.2.12 --description update",
    "push:android-staging": "appcenter codepush release-react -a HarmonyPayment/harmonypay-Android -d Staging -t 2.2.12 --description update",
    "push-clean:android-staging": "appcenter codepush deployment clear -a HarmonyPayment/harmonypay-Android Staging",
    "open:android": "open -a '/Applications/Android Studio.app' android",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "build-android": "rm -rf ~/.gradle/caches && cd android && ./gradlew --stop && ./gradlew cleanBuildCache && ./gradlew bundleRelease && cd ..",
    "remove:web": "grep -r UIWebView node_modules/*",
    "run:android-staging": "npx react-native run-android --variant=staging",
    "run:android-production": "npx react-native run-android --variant=release",
    "run:ios-staging-device": "npx react-native run-ios --scheme='staging' --device 'iPhone'",
    "run:ios-staging": "npx react-native run-ios --scheme=\"staging\" --simulator=\"iPhone 12 Pro Max\"",
    "run:ios-production": "npx react-native run-ios --scheme=\"harmonypay\" --simulator=\"iPhone 11\"",
    "open:iPhone": "open -a Simulator --args -CurrentDeviceUDID  53F9D553-5104-45BD-B8FF-B51315D499EC",
    "pod": "npx pod-install",
    "android:fix": "npx jetify",
    "clean:ios": "cd ios && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && rm -rf Podfile.lock"
  },
